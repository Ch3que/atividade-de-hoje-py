n= int(input("digite um numero"))
soma= 0
for item in range (2,n+1): 
    if item == 2 or item ==3 or item ==5 or item % 2 !=0 and item// item == 1 and item// 1 == item and item% 5 !=0 and item% 3 != 0:
        print (f'{item} é primo')
        soma+=item 
print(f"a soma dos numeros primos é igual {soma} ") 
